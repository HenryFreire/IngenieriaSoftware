public class Figuras {

    String nombre ;
    int numero;
    
    public String getNombre() {
        return nombre;
    }
    public void setNombre(String nombre) {
        this.nombre = nombre;
    }
    
    public int getNumero() {
        return numero;
    }
    
    public void setNumero(int numero) {
        this.numero = numero;
    }
    
      public Figuras(String nombre, int numero) {
        this.nombre = nombre;
        this.numero = numero;
    }
    
 }
